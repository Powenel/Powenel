  "author": {
    "name": "Hashversion",
    "email": "hello@hashversion.com",
    "url": "https://hashversion.com"
  },
  "type": "module",
  "scripts": {
    "build": "turbo build",
    "build:ui": "turbo build --filter=@repo/ui",
    "build:web": "turbo build --filter=web...",
    "clean": "turbo clean",
    "commit": "commit",
    "dev": "turbo dev",
    "dev:ui": "turbo dev --filter=@repo/ui",
    "dev:web": "turbo dev --filter=web...",
    "knip": "knip",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "pre-commit": "lint-staged",
    "prepare": "husky",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,mdx}\"",
    "prettier:format": "prettier --write \"**/*.{ts,tsx,mdx}\"",
    "start:web": "turbo start --filter=web"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/prompt-cli": "^19.8.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@types/node": "^24.3.1",
    "husky": "^9.1.7",
    "knip": "^5.64.0",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "sort-package-json": "^3.4.0",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2"
  },
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": ">=20"
  }
}
